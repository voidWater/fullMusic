/* pages/main/main.wxss */
.mybody{
  width: 100%;
  height:100%;
  background: url('https://www.fullmusic.club:444/img/main.jpg');
  background-repeat:no-repeat; background-size:100% 100%;-moz-background-size:100% 100%;
}
.head{
  margin-top: 100rpx;
  text-align: center;
  font-size: 70rpx;
  font-weight: 100rpx;
}
.btn-list{
  padding: 50rpx;
}
.btn{
  height:75rpx;
  text-align: center;
  margin: 25rpx 0;
  background-color:rgb(9, 179, 128);
  color: #fff;
  line-height: 75rpx;
  border-radius: 15rpx;
}
.jump-btn{
  
  position: fixed;
  right: 20rpx;
  bottom: 30rpx;
  padding: 20rpx;
  border: 1rpx solid rgb(185, 174, 174);
  color: rgb(202, 187, 187);
  font-size: 30rpx;
  border-radius: 50rpx;
  text-align: center;
}